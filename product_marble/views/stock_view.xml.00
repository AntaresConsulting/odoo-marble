<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
       
        <!-- views -->
        <record id="view_marble_move_form" model="ir.ui.view">
            <field name="name">marble.move.form</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_form"/>
            <field name="arch" type="xml">
               <field name="product_id" position="after">
                    <field name="is_raw" invisible="1" />
                    <field name="dimension_id" 
                        on_change="onchange_dimension_id(product_id, dimension_id)"
                        attrs="{'required':[('is_raw','=',True)], 'invisible':[('is_raw','=',False)]}" 
                    />
                    <field name="dimension_qty" 
                        on_change="onchange_dimension_qty(dimension_id, dimension_qty)"
                        attrs="{'required':[('is_raw','=',True)], 'invisible':[('is_raw','=',False)]}" 
                    />
                </field>
                <field name="name" position="replace"/>
                <group name="origin_grp" position="after">
                    <group name="description_name" colspan="6" col="6">
                        <field name="name" colspan="6"/>
                    </group>
                </group>
                <field name="product_qty" position="replace">
                    <field name="product_qty"
                        attrs="{'readonly':[('is_raw','=',True)]}" 
                        on_change="onchange_quantity(product_id, product_qty, product_uom, product_uos)"
                        class="oe_inline"
                    />
                </field>
                <field name="product_uom" position="replace">
                    <field name="product_uom" 
                        attrs="{'readonly':[('is_raw','=',True)]}" 
                        groups="product.group_uom" class="oe_inline"
                    />
                </field>
                <group name="origin_grp" position="replace">
                    <group name="origin_grp" string="Origin">
                        <field name="origin"             attrs="{'invisible': [('picking_id', '=', False)]}" readonly="1"/>
                        <field name="picking_id"         domain="[('type','=',type)]"                        />
                        <field name="type"               on_change="onchange_move_type(type)"                readonly="0"/>
                        <field name="location_id"        groups="stock.group_locations"                      readonly="0"/>
                        <field name="create_date"        groups="base.group_no_one"                          readonly="1"/>
                        <field name="partner_picking_id" attrs="{'readonly': [('state', '=', 'done')]}" 
                            domain="[('type','=','in')]"    
                        />
                    </group>
                </group>
            </field>
        </record>

  </data>
</openerp>
