<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- Product UOM Category -->
        <record id="product_uom_categ_area" model="product.uom.categ">
            <field name="name">Surface / Area</field>
        </record>

        <!-- Product UOM -->
        <record id="product_uom_square_meter" model="product.uom">
            <field name="category_id" ref="product_marble.product_uom_categ_area"/>
            <field name="name">m2</field>
            <field name="factor" eval="1.0"/>
        </record>

        <!-- Product Category -->
        <record id="prod_categ_raw_material" model="product.category">
            <field name="name">Raw Material</field>
            <field name="prod_type">raw</field>
        </record>
 
        <record id="prod_categ_bachas" model="product.category">
            <field name="name">Bachas</field>
            <field name="prod_type">bacha</field>
        </record>
 
         <record id="prod_categ_services" model="product.category">
            <field name="name">Services</field>
            <field name="prod_type">service</field>
        </record>
 
        <record id="prod_categ_inputs" model="product.category">
            <field name="name">Inputs</field>
            <field name="prod_type">input</field>
        </record>

<!-- Locations Definition -->

	

<!-- Picking Sequence -->

        <record id="seq_picking_internal" model="ir.sequence">
            <field name="name">Picking INT</field>
            <field name="code">stock.picking</field>
            <field name="prefix">PROD/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="stock.picking_type_internal" model="stock.picking.type">
            <field name="sequence_id" ref="product_marble.seq_picking_internal" />
        </record>

        <record id="seq_picking_outgoing" model="ir.sequence">
            <field name="name">Picking OUT</field>
            <field name="code">stock.picking.out</field>
            <field name="prefix">REMITO/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="stock.picking_type_out" model="stock.picking.type">
            <field name="sequence_id" ref="product_marble.seq_picking_outgoing" />
        </record>

        <record id="seq_picking_incoming" model="ir.sequence">
            <field name="name">Picking IN</field>
            <field name="code">stock.picking.in</field>
            <field name="prefix">PEDIDO/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="stock.picking_type_in" model="stock.picking.type">
            <field name="sequence_id" ref="product_marble.seq_picking_incoming" />
        </record>

 </data>
</openerp>
