            <field name="customer" eval="False"/>
            <field name="supplier" eval="False"/>
            <field name="lang">es_AR</field>
        </record>

        <record id="anton_ana" model="res.partner">
            <field name="name">Ana</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="email">ana@marmolesanton.com.ar</field>
            <field name="customer" eval="False"/>
            <field name="supplier" eval="False"/>
           <field name="lang">es_AR</field>
        </record>

        <!-- Users -->

        <record id="user_david" model="res.users">
            <field name="partner_id" ref="product_marble.anton_david"/>
            <field name="login">david</field>
            <field name="password">david</field>
            <field name="signature">David</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('group_marble_mger')])]"/>
        </record>

        <record id="user_erica" model="res.users">
            <field name="partner_id" ref="product_marble.anton_erica"/>
            <field name="login">erica</field>
            <field name="password">erica</field>
            <field name="signature">Erica</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
      
        <record id="user_rosa" model="res.users">
            <field name="partner_id" ref="product_marble.anton_rosa"/>
            <field name="login">rosa</field>
            <field name="password">rosa</field>
            <field name="signature">Rosa</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="groups_id" eval="[(6,0,[ref('base.group_hr_manager'),
                                                 ref('group_purchase_adminis'),
            ])]"/>
       </record>

        <record id="user_ana" model="res.users">
            <field name="partner_id" ref="product_marble.anton_ana"/>
