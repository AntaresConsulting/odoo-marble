<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data noupdate="0">

    <!-- User -->
    <record id="group_marble_user" model="res.groups">
        <field name="name">User Marble</field>
        <field name="category_id" ref="base.module_category_warehouse_management"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>
    
    <!-- Responsable -->
    <record id="group_marble_resp" model="res.groups">
        <field name="name">Responsable Marble</field>
        <field name="category_id" ref="base.module_category_warehouse_management"/>
        <field name="implied_ids" eval="[(4, ref('group_marble_user'))]"/>
    </record>
    
    <!-- Cutter -->
    <record id="group_marble_cutt" model="res.groups">
        <field name="name">Cutter Marble</field>
        <field name="category_id" ref="base.module_category_warehouse_management"/>
        <field name="implied_ids" eval="[(4, ref('group_marble_user'))]"/>
    </record>
    
    <!-- Manager -->
    <record id="group_marble_mger" model="res.groups">
        <field name="name">Manager Marble</field>
        <field name="category_id" ref="base.module_category_warehouse_management"/>
        <field name="implied_ids" eval="[(4, ref('group_marble_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

</data>
</openerp>
