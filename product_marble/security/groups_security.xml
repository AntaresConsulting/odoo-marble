<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data noupdate="0">

    <!-- Module Category -->

    <record id="module_marble_category" model="ir.module.category">
        <field name="name">Product Marble</field>
        <field name="description">Products marbles definition.</field>
        <field name="parent_id" ref="base.module_category_warehouse_management"/>
    </record>

    <!-- Groups -->

    <record id="group_marble_user" model="res.groups">
        <field name="name">User Marble</field>
        <field name="category_id" ref="module_marble_category"/>
    </record>
    
    <record id="group_marble_resp" model="res.groups">
        <field name="name">Responsable Marble</field>
        <field name="category_id" ref="module_marble_category"/>
    </record>
    
    <record id="group_marble_cutt" model="res.groups">
        <field name="name">Cutter Marble</field>
        <field name="category_id" ref="module_marble_category"/>
    </record>
    
    <record id="group_marble_mger" model="res.groups">
        <field name="name">Manager Marble</field>
        <field name="category_id" ref="module_marble_category"/>
        <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
    </record>
    
    <record id="group_marble_adminis" model="res.groups">
        <field name="name">Administrative Marble</field>
        <field name="category_id" ref="module_marble_category"/>
        <field name="implied_ids" eval="[(6,0,[ref('base.group_hr_manager'),ref('product.group_uom')])]"/>
        <field name="menu_access" eval="[(6,0,[ref('base.menu_purchase_root')])]"/>
    </record>
